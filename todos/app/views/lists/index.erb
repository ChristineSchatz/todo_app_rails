<h1>Welcome to your Todos App</h1>
<button><a href="/lists/new">Create a new list!</a></button>
<br><br>
<div>
  <% @lists.each do |list| %>
    <div>
      <a href="/lists/<%= list.id %>"><%= list.name %></a>
      <a href="/lists/<%= list.id %>/edit">Edit</a>
      <%= link_to("Delete", list, method: :delete) %>
  <% end %>
</div>




