<h1>Here is your <%= @list.name %> list!</h1>


<%= form_for [@list, @task] do |f| %>
  <%= f.label :description %>
  <%= f.text_field :description %>
   <%= f.submit "Add Task!" %>
<% end %>

<br>
<br>

<div>
  <% @tasks.each do |task| %>
    <div>
      <%= task.description %>
      <br>
      <%= task.completed %>
    </div>
    <br>
    <button><a href="/lists/<%= @list.id %>/tasks/<%= task.id %>/edit">Edit</a></button>

 <% end %>
</div>
<br>
<br>




